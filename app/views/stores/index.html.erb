<p id="notice"><%= notice %></p>

<h1>Listing Stores</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Predator</th>
      <th>Prey</th>
      <th>Alpha</th>
      <th>Beta</th>
      <th>Gamma</th>
      <th>Delta</th>
      <th>Time</th>
      <th>Increment</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stores.each do |store| %>
      <tr>
        <td><%= store.name %></td>
        <td><%= store.predator %></td>
        <td><%= store.prey %></td>
        <td><%= store.alpha %></td>
        <td><%= store.beta %></td>
        <td><%= store.gamma %></td>
        <td><%= store.delta %></td>
        <td><%= store.time %></td>
        <td><%= store.timeincrement %></td>
        <td><%= store.user_id %></td>
        <td><%= link_to 'Show', store %></td>
        <td><%= link_to 'Edit', edit_store_path(store) %></td>
        <td><%= link_to 'Destroy', store, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store', new_store_path %>
